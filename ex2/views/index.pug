extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Lista de Plantas
    .w3-container
      table.w3-table-all
        tr
          th _id
          th Frequesia
          th Especie
          th Número de intervenções
        
        each l in plantas
          tr
            td
              a(href="/"+l._id)= l._id
            td= l.Freguesia
            td= l.Espécie
            td= l.NúmeroIntervenções
              
    footer.w3-container.w3-teal
      h5 Generated by shopList::EngWeb2023 in #{d}
